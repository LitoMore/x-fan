  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint-config-xo": "^0.25.0",
    "eslint-config-xo-react": "^0.17.0",
    "eslint-plugin-react": "^7.11.1",
    "xo": "^0.23.0"
  },
  "xo": {
    "parser": "babel-eslint",
    "envs": [
      "node",
      "browser"
    ],
    "extends": [
      "xo/esnext",
      "xo-react"
    ],
    "rules": {
      "import/no-unassigned-import": [
        2,
        {
          "allow": [
            "**/style/*.css",
            "**/*.css"
          ]
        }
      ]
    }
  }
}
