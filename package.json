{
  "name": "fanfou-f7",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "fanfou-sdk-browser": "^0.1.0",
    "framework7": "^3.4.2",
    "framework7-icons": "^0.9.1",
    "framework7-react": "^3.4.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "2.0.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
